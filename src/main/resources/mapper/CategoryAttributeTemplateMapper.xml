<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.csmall.product.mapper.CategoryAttributeTemplateMapper">
    <!--int insert( CategoryCategoryAttributeTemplate CategoryAttributeTemplate);;-->
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        insert into mall_pms.pms_category_attribute_template
        values (null, #{categoryId}, #{attributeTemplateId}, #{gmt_create}, #{gmt_modified})
    </insert>
    <!--int insertBatch(List<CategoryAttributeTemplate> CategoryAttributeTemplates);-->
    <insert id="insertBatch" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO mall_pms.pms_category_attribute_template
        VALUES
        <foreach collection="list" item="i" separator=",">
            (null, #{i.categoryId}, #{i.attributeTemplateId}, #{i.gmt_create}, #{i.gmt_modified})
        </foreach>
    </insert>
    <!--int deleteById(Long id);-->
    <delete id="deleteById">
        DELETE
        FROM mall_pms.pms_category_attribute_template
        WHERE id = #{id}
    </delete>
    <!--int deleteByIds(Long[] id);-->
    <delete id="deleteByIds">
        DELETE FROM mall_pms.pms_category_attribute_template
        WHERE id in(
        <foreach collection="array" item="id" separator=",">
            #{id}
        </foreach>
        )
    </delete>
    <!--int update(Long id);-->
    <update id="update">
        UPDATE mall_pms.pms_category_attribute_template
        <set>
            <if test="name!=null">pms_category.name=#{name}</if>
            <if test="pinyin!=null">#{parentId}</if>
            <if test="keywords!=null">#{keywords}</if>
            <if test="sort!=null">#{sort}</if>
            <if test="gmtCreate!=null">#{gmtCreate}</if>
            <if test="gmtModified!=null">#{gmtModified}</if>

        </set>
        WHERE id=#{id}
    </update>
    <!--int count();-->
    <select id="count" resultType="int">
        SELECT COUNT(*)
        FROM mall_pms.pms_category_attribute_template
    </select>
    <!--int countByName(String name);-->
    <select id="countByName" resultType="int">
        SELECT COUNT(*) FROM mall_pms.pms_category_attribute_template WHERE id=#{id}
    </select>
    <!--CategoryAttributeTemplateStanderVO selectById(Long id);-->
    <select id="selectById" resultType="cn.tedu.csmall.product.pojo.vo.CategoryAttributeTemplateStanderVO">
        SELECT id, category_id, attribute_template_id
        FROM mall_pms.pms_category_attribute_template
        WHERE id = #{id}
    </select>
    <!--List<CategoryAttributeTemplateListVO> list();-->
    <select id="list" resultType="cn.tedu.csmall.product.pojo.vo.CategoryAttributeTemplateListVO">
        SELECT id, category_id, attribute_template_id
        FROM mall_pms.pms_category_attribute_template
        ORDER BY id DESC, id DESC
    </select>
</mapper>