<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.csmall.product.mapper.BrandMapper">
    <!--int insert(Album album);-->
    <insert id="insert">
        insert into mall_pms.pms_brand
        values (
                null,#{name},#{pinyin},#{logo},#{description},#{keywords},
                   #{sort},#{sales}, #{productCount},#{commentCount},
                   #{positiveCommentCount},#{enable},#{gmtCreate}, #{gmtModified}
               )
    </insert>
</mapper>
